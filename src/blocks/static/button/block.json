{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/button",
	"title": "Button",
	"category": "design",
	"icon": "button",
	"description": "Default button that follows theme global styling",
	"keywords": [ "button" ],
	"textdomain": "button",
	"supports": {
		"className": false
	},
	"editorScript": "file:../../../../build/js/button/button.js",
    "editorStyle": "file:../../../../build/css/button.css",
    "style": "file:../../../../build/css/style-button.css",
	"attributes": {
		"linkURL": {
			"type": "string",
			"default": ""
		},
		"linkTitle": {
			"type": "string",
			"default": ""
		},
		"linkText": {
			"type": "string",
			"default": "Read More"
		},
		"hasLinkTarget": {
			"type": "boolean"
		},
		"isPrimary": {
			"type": "boolean",
			"default": true
		},
		"isSecondary": {
			"type": "boolean",
			"default": false
		},
		"hasOutline": {
			"type": "boolean",
			"default": false
		},
		"hasTracking": {
			"type": "boolean",
			"default": false
		},
		"trackingIdentifier": {
			"type": "string",
			"default": ""
		}
	}
}
