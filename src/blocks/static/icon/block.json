{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/icon",
	"title": "Icon",
	"category": "media",
	"icon": "format-image",
	"description": "Add custom images or icon library classes (Font Awesome, Dashicons etc.)",
	"keywords": ["icon, icons"],
	"textdomain": "kotisivu-theme-blocks",
	"supports": {
		"className": false
	},
	"parent": ["ksd/icon-container"],
	"editorScript": "file:../../../../build/js/icon/icon.js",
	"editorStyle": "file:../../../../build/css/icon.css",
	"style": "file:../../../../build/css/style-icon.css",
	"attributes": {
		"iconClass": {
			"type": "string",
			"default": "fab fa-facebook"
		},
		"hasCustomIcon": {
			"type": "boolean"
		},
		"iconToggle": {
			"type": "boolean"
		},
		"linkURL": {
			"type": "string",
			"default": ""
		},
		"linkTitle": {
			"type": "string",
			"default": ""
		},
		"linkToggle": {
			"type": "boolean"
		},
		"mediaID": {
			"type": "number",
			"default": 0
		},
		"mediaURL": {
			"type": "string",
			"default": ""
		},
		"mediaALT": {
			"type": "string",
			"default": ""
		},
		"mediaMime": {
			"type": "string",
			"default": ""
		},
		"mediaWidth": {
			"type": "number"
		},
		"mediaHeight": {
			"type": "number"
		},
		"mediaClass": {
			"type": "string",
			"default": "ksd-icon"
		}
	}
}