{
	"name": "ksd/image-optimized",
	"attributes": {
		"mediaID": {
			"type": "number",
			"default": 0
		},
		"mediaURL": {
			"type": "string",
			"default": ""
		},
		"mediaALT": {
			"type": "string",
			"default": ""
		},
		"mediaWidth": {
			"type": "number"
		},
		"mediaHeight": {
			"type": "number"
		},
		"mediaMime": {
			"type": "string",
			"default": ""
		},
		"mediaClass": {
			"type": "string",
			"default": "image-block"
		},
		"sizes": {
			"type": "array",
			"source": "query",
			"selector": "picture",
			"default": [],
			"query": {
				"id": {
					"type": "number",
					"source": "attribute",
					"attribute": "data-id",
					"selector": "source"
				},
				"url": {
					"type": "string",
					"source": "attribute",
					"attribute": "src",
					"selector": "source"
				},
				"width": {
					"type": "number",
					"source": "attribute",
					"attribute": "width",
					"selector": "source"
				},
				"height": {
					"type": "number",
					"source": "attribute",
					"attribute": "height",
					"selector": "source"
				}
			}
		},
		"lazyLoad": {
			"type": "boolean",
			"default": true
		}
	}
}
