{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/address",
	"title": "Address Container",
	"category": "design",
	"icon": "columns",
	"description": "Optimized column for column container",
	"keywords": ["address, container, email, phone"],
	"textdomain": "kotisivu-theme-blocks",
	"supports": {
		"className": false
	},
	"editorScript": "file:../../../../build/js/address/address.js",
	"editorStyle": "file:../../../../build/css/address.css",
	"style": "file:../../../../build/css/style-address.css",
	"attributes": {
		"wrapperToggle": {
			"type": "boolean"
		},
		"wrapperClass": {
			"type": "string",
			"default": "address"
		},
		"template": {
			"type": "array",
			"default": []
		},
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", false ]
		}
	}
}
