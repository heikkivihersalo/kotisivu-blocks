		"linkTitle": {
			"type": "string",
			"default": ""
		},
		"linkText": {
			"type": "string",
			"default": ""
		},
		"linkTarget": {
			"type": "string"
		},
		"linkRel": {
			"type": "string"
		},
		"iconClass": {
			"type": "string",
			"default": "fas fa-phone"
		},
		"iconToggle": {
			"type": "boolean",
			"default": true
		},
		"hasTracking": {
			"type": "boolean",
			"default": false
