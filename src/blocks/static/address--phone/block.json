{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/address--phone",
	"title": "Phone",
	"category": "design",
	"icon": "phone",
	"description": "Simple phone field for contact information",
	"keywords": ["phone, tel"],
	"textdomain": "kotisivu-theme-blocks",
	"supports": {
		"className": false
	},
	"parent": ["ksd/address"],
	"editorScript": "file:../../../../build/js/address--phone/address--phone.js",
	"editorStyle": "file:../../../../build/css/address--phone.css",
